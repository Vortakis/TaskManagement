{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=taskdb.sqlite"
  },
  "TaskManagement": {
    "TaskPrioritySection": {
      "UrgentDaysLimt": 1,
      "NormalDaysLimit": 3
    },
    "TaskCompletionSection": {
      "EarlyCompletionLimit": 3
    },
    "ConcurrentProcessingSection": {
      "ParallelismDegree": 10,
      "BatchSize": 1000,
      "MaxRetries": 3,
      "RetryDelayMS": 100
    }
  }
}
